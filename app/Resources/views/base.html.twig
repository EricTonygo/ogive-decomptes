<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
        <title>{% block title %}{% endblock %} - OGIVE DC</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/semantic-ui/2.2.10/semantic.min.css">
            <link rel="stylesheet" href="{{ asset('bundles/ogiveproject/stylesheets/styles.css') }}">
            <link rel="stylesheet" href="{{ asset('bundles/ogiveproject/css/jquery.datetimepicker.min.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    {% block body %}{% endblock %}
    {% block javascripts %}
        <script
                src="https://code.jquery.com/jquery-3.1.1.min.js"
                integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
            crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/semantic-ui/2.2.10/semantic.min.js"></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/ogiveproject/js/main.js')}}"></script>
            <script type="text/javascript" src="{{ asset('bundles/ogiveproject/js/jquery.datetimepicker.full.min.js')}}"></script>
        {% endblock %}
    </body>
</html>
